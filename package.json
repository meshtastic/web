    "@types/web-bluetooth": "^0.0.16",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.12",
    "gzipper": "^7.1.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rollup-plugin-visualizer": "^5.8.3",
    "tailwindcss": "^3.2.1",
    "tar": "^6.1.11",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "unimported": "^1.22.0",
