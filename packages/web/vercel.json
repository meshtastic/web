{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"rewrites": [
		{
			"source": "/((?!api|.*\\..*).*)",
			"destination": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/",
			"headers": [
				{
					"key": "Cross-Origin-Embedder-Policy",
					"value": "require-corp"
				},
				{
					"key": "Cross-Origin-Opener-Policy",
					"value": "same-origin"
				}
			]
		}
	],
	"relatedProjects": ["prj_G5osDwzRZBGplkfYTGFQziUGbLOm"]
}
